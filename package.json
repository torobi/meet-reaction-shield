{
  "name": "meet-reaction-shield",
  "description": "Restrict or confirm reactions in Google Meet for better meeting control.",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "author": "torobi",
  "repository": {
    "type": "git",
    "url": "https://github.com/torobi/meet-reaction-shield.git"
  },
  "homepage": "https://github.com/torobi/meet-reaction-shield",
  "bugs": {
    "url": "https://github.com/torobi/meet-reaction-shield/issues"
  },
  "keywords": [
    "chrome-extension",
    "google-meet",
    "emoji",
    "reaction",
    "svelte"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
